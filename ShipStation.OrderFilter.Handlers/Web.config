<?xml version="1.0" encoding="utf-8"?>
<!--
  For more information on how to configure your ASP.NET application, please visit
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>

  <location path="." inheritInChildApplications="false">
    <appSettings configSource="appSettings.config" />

    <system.web>

      <httpRuntime targetFramework="4.5.1"/>

      <customErrors mode="Off" />

      <compilation debug="true" targetFramework="4.5.1">
        <assemblies>
          <add assembly="System.Diagnostics.DiagnosticSource, Version=4.0.2.1, Culture=neutral, PublicKeyToken=cc7b13ffcd2ddd51"/>
        </assemblies>
      </compilation>

      <pages>
        <namespaces>
        </namespaces>
        <controls />
      </pages>

    </system.web>

  </location>

  <system.webServer>

    <httpProtocol>
      <customHeaders>
        <add name="Access-Control-Allow-Origin" value="*" />
        <add name="Access-Control-Allow-Headers" value="Authorization" />
        <add name="X-Frame-Options" value="ALLOWALL" />
      </customHeaders>
    </httpProtocol>

    <staticContent>
      <remove fileExtension=".woff" />
      <mimeMap fileExtension=".woff" mimeType="application/font-woff" />
      <remove fileExtension=".ttf" />
      <mimeMap fileExtension=".ttf" mimeType="application/x-font-ttf" />
      <remove fileExtension=".svg" />
      <mimeMap fileExtension=".svg" mimeType="image/svg+xml" />
      <remove fileExtension="" />
      <mimeMap fileExtension="" mimeType="text/plain" />
    </staticContent>

    <defaultDocument>
      <files>
        <clear />
        <add value="index.html" />
        <add value="default.aspx" />
        <add value="index.htm" />
        <add value="default.html" />
        <add value="Default.htm" />
      </files>
    </defaultDocument>

  </system.webServer>

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Diagnostics.DiagnosticSource" culture="neutral" publicKeyToken="cc7b13ffcd2ddd51"/>
        <bindingRedirect oldVersion="0.0.0.0-4.0.2.1" newVersion="4.0.2.1"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>

</configuration>